{{> partial_list_header }}

<div class="row search-inputs" data-name="{{dataName}}" data-url="{{searchUrl}}">
	<div class="col-xs-12 col-sm-4">
		<div class="form-group has-feedback">
			<input type="text" id="searchInput1" name="name" class="form-control search-input"
				placeholder="Filter list by name" title="Space seperated patterns" class="form-control">
			<span class="glyphicon glyphicon-search form-control-feedback"></span>
		</div>
	</div>
	<div class="col-xs-12 col-sm-4">
		<div class="form-group has-feedback">
			<input type="text" id="searchInput2" name="email" class="form-control search-input"
				placeholder="Filter list by email" title="Space seperated patterns" class="form-control">
			<span class="glyphicon glyphicon-search form-control-feedback"></span>
		</div>
	</div>
	<div class="col-xs-12 col-sm-4">
		<div class="form-group has-feedback">
			<input type="text" id="searchInput3" name="level" class="form-control search-input"
				placeholder="Filter list level" title="Space seperated patterns" class="form-control">
			<span class="glyphicon glyphicon-search form-control-feedback"></span>
		</div>
	</div>
</div>

{{> partial_list_footer }}
<script src="/js/list-users.js"></script>